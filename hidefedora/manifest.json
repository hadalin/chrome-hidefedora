{
  "name": "Hide Fedora",
  "version": "1.7.0",
  "manifest_version": 2,
  "description": "Removes fedora Youtube commenters.",
  "homepage_url": "http://hidefedora.hadal.in",
  "icons": {
    "48": "icons/icon48-text.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://plus.googleapis.com/*",
        "*://apis.google.com/*",
        "*://jhvisser.com/hidefedora/*"
      ],
      "all_frames": true,
      "run_at": "document_end",
      "css": [
        "src/content.css"
      ],
      "js": [
        "assets/jquery/jquery.min.js",
        "assets/underscore/underscore-min.js",
        "assets/moment/moment.min.js",
        "src/content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "resources/*"
  ],
  "permissions": [
    "storage"
  ],
  "options_page": "options/options.html"
}
